{

  "name": "Clean Feed",
  "description": "Removes all shared content from Facebook.",
  "version": "1.0.0",
  "content_scripts": [
    {
      "matches": ["http://www.facebook.com/*", "https://www.facebook.com/*"],
      "js": ["jquery.min.js", "remove_shared.js"]
    }
  ],
  "permissions": [
    "tabs", "activeTab", "<all_urls>"
  ],
  "manifest_version": 2
}
